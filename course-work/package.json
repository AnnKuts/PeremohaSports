{
  "name": "express-api-starter-ts",
  "version": "2.0.0",
  "description": "A basic starter for an express.js v5 API with Typescript",
  "author": "CJ R. <cj@null.computer> (https://w3cj.now.sh)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w3cj/express-api-starter-ts.git"
  },
  "keywords": [],
  "main": "src/index.ts",
  "scripts": {
    "start": "node --env-file=.env dist/src/index.js",
    "dev": "tsx watch --env-file=.env src/index.ts",
    "build": "tsc",
    "lint": "eslint --fix src tests",
    "test": "vitest run",
    "test:u": "vitest run tests/unit",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:all": "npm run test:u && npm run test:i",
    "pretest:i": "dotenv -e .env.test -- docker compose -f ../docker-compose.test.yaml up -d && echo 'good luck with testing' && sleep 10 && dotenv -e .env.test -- npx prisma generate && dotenv -e .env.test -- npx prisma migrate reset --skip-generate --force",
    "test:i": "dotenv -e .env.test -- vitest run tests/integration --no-file-parallelism",
    "dev:full": "docker compose up -d &&  npx prisma migrate generate && npx prisma migrate deploy && npx prisma db seed && npx tsx prisma/insert-admin.ts && npm run dev",
    "posttest:i": "docker compose down",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^6.19.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "nodemailer": "^6.9.16",
    "pg": "^8.16.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@prisma/language-server": "^31.1.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.0.2",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.38.0",
    "eslint-plugin-format": "^1.0.2",
    "prisma": "^6.19.0",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.16",
    "vitest-mock-extended": "^3.1.0"
  },
  "directories": {
    "test": "tests"
  },
  "bugs": {
    "url": "https://github.com/w3cj/express-api-starter-ts/issues"
  },
  "homepage": "https://github.com/w3cj/express-api-starter-ts#readme"
}