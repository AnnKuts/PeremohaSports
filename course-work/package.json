{
  "name": "express-api-starter-ts",
  "version": "2.0.0",
  "description": "A basic starter for an express.js v5 API with Typescript",
  "author": "CJ R. <cj@null.computer> (https://w3cj.now.sh)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w3cj/express-api-starter-ts.git"
  },
  "keywords": [],
  "main": "src/index.ts",
  "scripts": {
    "start": "node --env-file=.env dist/src/index.js",
    "dev": "tsx watch --env-file=.env src/index.ts",
    "build": "tsc",
    "lint": "eslint --fix src tests",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run --config vitest.integration.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "nodemailer": "^6.9.16",
    "pg": "^8.16.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@prisma/language-server": "^31.1.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.9.2",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.38.0",
    "eslint-plugin-format": "^1.0.2",
    "prisma": "^6.19.0",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  },
  "directories": {
    "test": "tests"
  },
  "bugs": {
    "url": "https://github.com/w3cj/express-api-starter-ts/issues"
  },
  "homepage": "https://github.com/w3cj/express-api-starter-ts#readme"
}
